package ${package.mapper};

#foreach($pkg in ${importMapperFrameworkPackages})
import ${pkg};
#end
#if($importMapperJavaPackages.size()>0)

#foreach($pkg in ${importMapperJavaPackages})
import ${pkg};
#end
#end

/**
 * $!{table.comment} Mapper 接口
 *
 * @author ${author}
 * @since ${date}
 */
#if(${mapperAnnotationClass})
@${mapperAnnotationClass.simpleName}
#end
#if(${enhancer})
public interface ${mapper} extends ${superMapperClass}<${entity}>, EnhancedMapper<${queryVO}> {
#else
public interface ${mapper} extends ${superMapperClass}<${entity}> {
#end
#if(${generateQuery})

    #if(${enhancer})
    @Override
    #end
    List<${queryVO}> voQueryByXml(Object param, IPage<${queryVO}> page);

    #if(${enhancer})
    @Override
    #end
    default List<${queryVO}> voQuery(Object param, IPage<${queryVO}> page) {
        #if(${enhancer})
        return EnhancedMapper.super.voQuery(param, page);
        #else
        return this.voQueryByXml(param, page);      
        #end
    }
#end
}
